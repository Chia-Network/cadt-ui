{
  "name": "cadt-ui",
  "version": "1.2.9",
  "private": true,
  "author": "Chia Network Inc. <hello@chia.net>",
  "homepage": "./",
  "main": "src/start.js",
  "engineStrict": true,
  "engines": {
    "node": ">=18.16.0"
  },
  "dependencies": {
    "@date-io/dayjs": "^3.0.0",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@lukeed/uuid": "^2.0.1",
    "@mui/lab": "^5.0.0-alpha.162",
    "@mui/material": "^5.15.7",
    "@mui/x-date-pickers": "^6.19.2",
    "dayjs": "^1.11.10",
    "formik": "^2.4.5",
    "lodash": "^4.17.21",
    "qrcode.react": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-indiana-drag-scroll": "^2.2.0",
    "react-intl": "^6.6.2",
    "react-notifications": "^1.7.4",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.21.3",
    "react-scripts": "^5.0.1",
    "react-select": "^5.8.0",
    "redux": "^5.0.1",
    "redux-thunk": "^3.1.0",
    "socket.io-client": "^4.7.4",
    "styled-components": "^6.1.8",
    "update-notifier": "^7.0.0",
    "updeep": "^1.2.4",
    "web-vitals": "^3.5.2",
    "yup": "^1.3.3"
  },
  "scripts": {
    "start": "nf start -p 31329",
    "build": "react-scripts build",
    "test": "mocha tests/**/*.spec.js --reporter spec --exit",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "electron-start": "node --trace-warnings src/start-react",
    "react-start": "react-scripts start",
    "pack": "build --dir",
    "dist": "npm run build && build",
    "requirements-check": "node check_node_version.js",
    "postinstall": "npm run requirements-check && install-app-deps",
    "release": "",
    "prepare": "husky install",
    "electron:package:mac": "npm run build && electron-builder -m -c.extraMetadata.main=build/electron.js",
    "electron:package:win": "npm run build && electron-builder -w -c.extraMetadata.main=build/electron.js",
    "electron:package:linux": "npm run build && electron-builder -l -c.extraMetadata.main=build/electron.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      "last 1 electron version",
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 electron version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.23.9",
    "@babel/eslint-parser": "^7.23.10",
    "@babel/plugin-proposal-private-property-in-object": "7.21.11",
    "@babel/preset-env": "^7.23.9",
    "@babel/register": "^7.23.7",
    "@commitlint/cli": "^18.6.0",
    "@commitlint/config-conventional": "^18.6.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-react-app": "^10.0.1",
    "chai": "^5.0.3",
    "dotenv-webpack": "^8.0.1",
    "electron": "^28.2.0",
    "electron-builder": "^24.9.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-mocha": "^10.2.0",
    "eslint-plugin-react": "^7.33.2",
    "foreman": "^3.0.1",
    "husky": "^9.0.7",
    "mocha": "^10.2.0",
    "mock-local-storage": "^1.1.24",
    "prettier": "^3.2.4",
    "react-app-env": "^1.2.3",
    "react-error-overlay": "6.0.11",
    "standard-version": "^9.5.0"
  },
  "build": {
    "appId": "org.worldbank.climate-action-data-trust",
    "productName": "cadt-ui",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "public"
    },
    "mac": {
      "target": {
        "target": "dmg",
        "arch": [
          "universal"
        ]
      },
      "publish": null,
      "icon": "src/assets/img/CADT_Icon.png"
    },
    "win": {
      "target": "nsis",
      "publish": null,
      "icon": "src/assets/img/CADT_Icon.png"
    },
    "linux": {
      "target": "deb",
      "description": "Climate Action Data Trust user interface desktop application. Allows point-and-click interface for the CADT API application.",
      "maintainer": "Chia Network Inc <hello@chia.net>",
      "vendor": "https://climateactiondata.org/",
      "publish": null,
      "icon": "src/assets/img/CADT_Icon.png",
      "desktop": {
        "Icon": "/usr/share/icons/hicolor/0x0/apps/cadt-ui.png"
      }
    }
  },
  "standard-version": {
    "skip": {
      "bump": true,
      "commit": true,
      "tag": true
    }
  },
  "contributors": [
    "Daniel <daniel.stefan@equilobe.com>",
    "Stanley Pageot <stanley@rxdu.io>",
    "Michael Taylor <5665004+MichaelTaylor3D@users.noreply.github.com>",
    "Michael.Taylor <mtaylor@michaeltaylor3d.com>",
    "Daniel Stefan <82083960+danielstefanequilobe@users.noreply.github.com>",
    "SPageot <Stanley.Pageot@hotmail.com>",
    "Justin England <justin@jeng.land>",
    "Zachary Brown <zachary.g.brown@gmail.com>"
  ]
}
